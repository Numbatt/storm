# Flood Risk AI Environment Variables
# Copy this file to .env and add your actual API keys

# Google Maps API Keys (can be the same key for all Google services)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
GOOGLE_STREET_VIEW_API_KEY=your_google_street_view_api_key_here
GOOGLE_ELEVATION_API_KEY=your_google_elevation_api_key_here

# OpenWeather API Key (get from https://openweathermap.org/api)
OPENWEATHER_API_KEY=your_openweather_api_key_here

# Backend Configuration
PORT=3001
